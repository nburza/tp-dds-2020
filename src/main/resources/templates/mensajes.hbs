{{#partial "contenido"}}

    <div>
        <h2>Mensajes</h2><br>
        <table class="table table-striped table-hover" style="width:100%">
            <thead class="thead-dark">
            <tr>
                <th scope="col">Egreso ID</th>
                <th scope="col">Fecha</th>
                <th scope="col">Importe Total</th>
                <th scope="col">Moneda</th>
                <th scope="col">Estado</th>
            </tr>
            </thead>
            {{#each egresos}}
                <tr>
                    <td>{{id}}</td>
                    <td>{{fecha}}</td>
                    <td>{{importeTotal}}</td>
                    <td>{{moneda}}</td>
                    <td>{{estado}}</td>
                </tr>
            {{/each}}

        </table>
    </div>

{{/partial}}

{{>layout.hbs}}

